		},
		"parameters": {
			"ds_p_schema": {
				"type": "string"
			},
			"ds_p_table": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "AzureSqlTable",
		"schema": [],
		"typeProperties": {
			"schema": {
				"value": "@split(dataset().ds_p_schema,'/')[2]",
